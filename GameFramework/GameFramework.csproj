<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
		<OutputType>Library</OutputType>
        <TargetFramework>netcoreapp3.1</TargetFramework>
		<OutputPath>..\..\..\Temp\bin\Tools\Debug\</OutputPath>
		<BaseIntermediateOutputPath>..\..\..\Temp\obj\$(MSBuildProjectName)\</BaseIntermediateOutputPath>
		<UseHostCompilerIfAvailable>false</UseHostCompilerIfAvailable>
		<RootNamespace>GameFramework</RootNamespace>
    </PropertyGroup>

    <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
      <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    </PropertyGroup>

    <ItemGroup>
		<Compile Remove="Editor\**" />
		<Compile Remove="obj\**" />
		<EmbeddedResource Remove="Editor\**" />
		<EmbeddedResource Remove="obj\**" />
		<None Remove="**/*.meta" />
		<None Remove="Editor\**" />
		<None Remove="obj\**" />
    </ItemGroup>

    <ItemGroup>
      <Compile Remove="Properties\AssemblyInfo.cs" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="GameFramework.asmdef" />
    </ItemGroup>

    <ItemGroup>
      <Folder Include="Properties\" />
    </ItemGroup>

</Project>
